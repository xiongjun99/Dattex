<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RollViewPager">
        <attr name="rollviewpager_hint_mode">
            <enum name="point" value="0" />
            <enum name="number" value="1" />
        </attr>
        <attr name="rollviewpager_hint_gravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="rollviewpager_hint_paddingRight" format="dimension"/>
        <attr name="rollviewpager_hint_paddingLeft" format="dimension"/>
        <attr name="rollviewpager_hint_paddingTop" format="dimension"/>
        <attr name="rollviewpager_hint_paddingBottom" format="dimension"/>

        <attr name="rollviewpager_play_delay" format="integer" />
        <attr name="rollviewpager_hint_color" format="color" />
        <attr name="rollviewpager_hint_alpha" format="integer" />
    </declare-styleable>

    <declare-styleable name="CommonTitleBar">

        <attr name="leftClick" format="reference" />
        <attr name="rightClick" format="reference" />
        <attr name="titleText" format="string" />
        <attr name="leftIcon" format="reference" />
        <attr name="rightIcon" format="reference" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="titleTextColor" format="color" />
    </declare-styleable>


    <declare-styleable name="RightViewArr">
        <attr name="arrowIcon" format="reference" />
        <attr name="titleContent" format="string" />
        <attr name="contentIcon" format="reference" />
        <attr name="contentColor" format="color" />
        <attr name="lineVisible" format="boolean" />
    </declare-styleable>
    <declare-styleable name="EditPop">
        <attr name="list" format="reference" />
    </declare-styleable>


    <declare-styleable name="LeftDrawableView">
        <attr name="leftContent" format="string" />
        <attr name="leftContentICon" format="reference" />
        <attr name="leftBackground" format="reference" />
        <attr name="leftContentColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SettingItemView">
        <attr name="leftText" format="string" />
        <attr name="rightText" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomSeekBar">
        <attr name="csb_progress_height" format="dimension" />
        <attr name="csb_progress_corner" format="dimension" />
        <attr name="csb_section_radius" format="dimension" />
        <attr name="csb_section_enable" format="boolean" />
        <attr name="csb_thumb_radius" format="dimension" />
        <attr name="csb_thumb_point" format="reference"/>
        <attr name="csb_background_color" format="color" />
        <attr name="csb_progress_color" format="color" />
        <attr name="csb_min_value" format="integer" />
        <attr name="csb_max_value" format="integer" />
        <attr name="csb_overspread_enable" format="boolean" />
        <attr name="csb_thumb_scale_enable" format="boolean" />
        <attr name="csb_thumb_anim_enable" format="boolean" />
        <attr name="csb_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
</resources>