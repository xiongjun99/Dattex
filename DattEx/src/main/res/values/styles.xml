<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="BaseAppTheme">
        <item name="android:windowBackground">@color/color_282C42</item>
    </style>

    <style name="SplashTheme" parent="BaseAppTheme">
        <item name="android:background">@drawable/shape_splash</item>
    </style>


    <style name="klineIndexWidget">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:textColor">@drawable/selector_text_color_checked_47b9fd_normal_edeef2
        </item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="klineTopInfoText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:textColor">@color/color_FFEDEEF2</item>
        <item name="android:textSize">14dp</item>

    </style>

    <style name="klineTopInfoLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_FFA3A6BF</item>
        <item name="android:textSize">14dp</item>

    </style>

    <style name="klineBottomNav">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:paddingTop">22dp</item>
        <item name="android:paddingBottom">22dp</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:gravity">center</item>
        <item name="background">@drawable/selector_check_box_background_under_line</item>
        <item name="android:textColor">@drawable/selector_text_color_check_ffffff_normal_767b99
        </item>
        <item name="android:textSize">14dp</item>

    </style>


